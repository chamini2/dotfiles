# Install native apps
brew tap phinze/homebrew-cask
brew install brew-cask

function installcask() {
    brew cask install "${@}"
}

installcask all2mp3
installcask android-file-transfer
installcask atom
installcask daisydisk # Matteo Ferrando, X3TSF4BJ-GZBWAN7B-NMJJMWSZ-8E36C6G9-WZJUK77L
installcask dropbox
installcask github
installcask google-chrome
installcask google-drive
installcask img2icns
installcask iterm2
installcask keka
installcask mamp
installcask pandoc # http://johnmacfarlane.net/pandoc/installing.html
installcask picasa
installcask qtspim
installcask rcdefaultapp
installcask skype
installcask transmission
installcask virtualbox
installcask vlc

# Cleanup
brew cleanup --force
rm -f -r /Library/Caches/Homebrew/*

